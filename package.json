{
  "type": "module",
  "version": "4.6.2",
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc && vite build --mode test",
    "serve": "http-server ./dist -o",
    "serve:test": "http-server ./dist-test -o",
    "svgo": "svgo -f src/assets/icons",
    "new": "plop",
    "generate:icons": "esno ./scripts/generate.icons.ts",
    "lint": "npm-run-all -s lint:tsc lint:eslint lint:stylelint",
    "lint:tsc": "vue-tsc",
    "lint:eslint": "eslint . --cache --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "commit": "git cz",
    "release": "bumpp"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@imengyu/vue3-context-menu": "^1.3.8",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "defu": "^6.1.4",
    "element-plus": "^2.6.1",
    "eruda": "^3.0.1",
    "floating-vue": "5.2.2",
    "hotkeys-js": "^3.13.7",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^2.6.1",
    "overlayscrollbars-vue": "^0.5.7",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.1.7",
    "qs": "^6.12.0",
    "scule": "^1.3.0",
    "vconsole": "^3.15.1",
    "vue": "^3.4.21",
    "vue-m-message": "^4.0.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.8.1",
    "@iconify/json": "^2.2.191",
    "@iconify/vue": "^4.1.1",
    "@stylistic/stylelint-config": "^1.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/path-browserify": "^1.0.2",
    "@types/qs": "^6.9.12",
    "@unocss/eslint-plugin": "^0.58.5",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "boxen": "^7.1.1",
    "bumpp": "^9.4.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "http-server": "^14.1.1",
    "inquirer": "^9.2.16",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "plop": "^4.0.1",
    "sass": "^1.72.0",
    "simple-git-hooks": "^2.10.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.2.1",
    "svgo": "^3.2.0",
    "terser": "^5.29.1",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-turbo-console": "^1.4.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-compression2": "^1.0.0",
    "vite-plugin-fake-server": "^2.1.1",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.0.17",
    "vite-plugin-vue-meta-layouts": "^0.4.2",
    "vue-tsc": "^2.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
